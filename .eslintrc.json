{
  "root": true,
  "extends": [
    "ponko2",
    "plugin:node/recommended"
  ],
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script",
    "ecmaFeatures": {
      "impliedStrict": false
    }
  },
  "plugins": ["node"]
}
